{
  "extends": "../../packages/tsconfig/nextjs.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ],
    },
    "strictNullChecks": true
  },
  "include": [
    "tsconfig.json",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
